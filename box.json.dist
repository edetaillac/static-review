{
    "chmod": "0755",
    "directories": [
        "src"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["bin", "doctrine", "phpdocumentor", "phpspec", "sebastian", "phpunit", "mockery", "Tests"],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "bin/precommit",
    "output": "precommit.phar",
    "stub": true
}
